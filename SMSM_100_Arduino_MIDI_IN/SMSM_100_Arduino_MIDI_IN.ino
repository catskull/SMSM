void writeSN76489(byte data) {
  
  PORTC = data & 0x0f;
  digitalWrite(hl, LOW); 
  digitalWrite(wr, HIGH); 
  delayMicroseconds(dT);
  digitalWrite(wr, LOW); 
  delayMicroseconds(dT);

  PORTC = (data >> 4) & 0x0f; 
  digitalWrite(hl, HIGH); 
  digitalWrite(wr, HIGH); 
  delayMicroseconds(dT);
  digitalWrite(wr, LOW); 
  delayMicroseconds(dT);    
}




