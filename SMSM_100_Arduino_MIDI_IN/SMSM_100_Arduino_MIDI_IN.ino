void setup() {
  Serial.begin(31250);
  // temp variable for flashing status LED on startup
  bool led = 0;

  DDRC = DDRC | B00111111; // set the direction for PORTC

  pinMode(wr, OUTPUT); 
  pinMode(hl, OUTPUT); 
  pinMode(fm, OUTPUT); 
  digitalWrite(fm, LOW);

  PORTC = 0;

  pinMode(MIDI_PANIC_BUTTON, INPUT_PULLUP);
  pinMode(STATUS_LED, OUTPUT);
  digitalWrite(STATUS_LED, HIGH);


  delay(6500);
  doCC(4, 14, 101);
  doCC(5, 14, 101);
  doCC(6, 14, 101);
  doCC(7, 14, 101);
  doCC(8, 14, 101);
  doCC(9, 14, 101);
  doCC(10, 14, 101);
  doCC(11, 14, 101);
  doCC(12, 14, 101);
  
  for(int j = 0; j < 4; j ++) {
      doNoteOn(0, (j * 7) + 45, 100);
      doNoteOn(1, (j * 7) + 52, 100);
      doNoteOn(2, (j * 7) + 59, 100);
      doNoteOn(3, (j * 7) + 40, 100);
      
