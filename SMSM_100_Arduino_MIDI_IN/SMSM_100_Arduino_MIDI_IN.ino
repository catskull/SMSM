        shadow[0x0e] = (shadow[0x0e] & B11101111) | (drum_key << 4);
        shadow[0x36] = (shadow[0x36] & B11110000) | (15 - (velo1 >> 3));
        
        writeYM2413(0x36, shadow[0x36]);
        writeYM2413(0x0e, shadow[0x0e]);
        
